{
  "name": "gsoi/resque",
  "description": "Resque PHP",
  "keywords": ["Asynchronous Task manager"],
  "homepage": "https://github.com/GSOinteractive/resque",
  "license": "MIT",
  "authors": [
    {
      "name": "William Pottier",
      "email": "developer@william-pottier.fr"
    },
    {
      "name": "AllProgrammic",
      "homepage": "https://allprogrammic.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "ext-redis": "*",
    "allprogrammic/redis-client": "1.0.x-dev",
    "psr/log": "*",
    "symfony/event-dispatcher": "^2.7 || ^3.0 || ^4.0",
    "symfony/dependency-injection": "^2.7 || ^3.0 || ^4.0"
  },
  "autoload": {
    "psr-4": {
      "AllProgrammic\\Component\\Resque\\": "src",
      "AllProgrammic\\Component\\Resque\\Tests\\": "tests"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "support": {
    "docs": "https://github.com/GSOinteractive/resque/wiki"
  },
  "replace": {
    "allprogrammic/resque": "*"
  }
}
